Item('token');\n\n\t\taxios\n\t\t\t.get(urlAcompanhante, {\n\t\t\t\theaders: {\n\t\t\t\t\tAuthorization: `Bearer ${token}`,\n\t\t\t\t},\n\t\t\t})\n\t\t\t.then(response => {\n\t\t\t\tif (response.status === 200) {\n\t\t\t\t\tconst stateResponse = response.data.address?.state;\n\t\t\t\t\tconst cityResponse = response.data.address?.city;\n\n\t\t\t\t\tsetPrice([response.data.profile?.price]);\n\t\t\t\t\tsetPaymentTypeValues(response.data.profile?.payment_type);\n\t\t\t\t\tsetState(stateResponse);\n\t\t\t\t\tlet idState = '0';\n\t\t\t\t\tmyStatesArray.forEach(element => {\n\t\t\t\t\t\tif (element.label === stateResponse) {\n\t\t\t\t\t\t\tidState = element.value;\n\t\t\t\t\t\t}\n\t\t\t\t\t});\n\t\t\t\t\tsetSelectsAxiosIbge('cidades', idState, cityResponse);\n\t\t\t\t\tsetNeighborhood(response.data.address?.neighborhood);\n\t\t\t\t\tsetCanLocal(response.data.profile?.can_local ? '1' : '0');\n\t\t\t\t\tsetOccasionValues(response.data.profile?.occasion);\n\t\t\t\t\tsetPreferencesValues(response.data.profile?.type_service);\n\t\t\t\t\tsetLanguagesValues(response.data.profile?.languages);\n\t\t\t\t\tsetUrlInstagram(response.data.profile?.url_instagram);\n\t\t\t\t\tsetUrlTikTok(response.data.profile?.url_tiktok);\n\t\t\t\t\tsetUrlTwitter(response.data.profile?.url_twitter);\n\t\t\t\t\tsetUrlLinktree(response.data.profile?.url_linktree);\n\t\t\t\t\tsetUrlOnlyfans(response.data.profile?.url_onlyfans);\n\t\t\t\t\tsetUrlWeb(response.data.profile?.url_web);\n\n\t\t\t\t\tconst fotos: valueFilesType = [];\n\t\t\t\t\tconst videos: valueFilesType = [];\n\n\t\t\t\t\tif (response.data.profile.midias.length > 0) {\n\t\t\t\t\t\tconst midiasProfile = response.data.profile.midias;\n\n\t\t\t\t\t\tmidiasProfile.forEach((midia: any) => {\n\t\t\t\t\t\t\tif (midia.type === 'image') {\n\t\t\t\t\t\t\t\tconst lastMidiaLink =\n\t\t\t\t\t\t\t\t\tmidia.link[\n\t\t\t\t\t\t\t\t\t\tObject.keys(midia.link)[Object.keys(midia.link).length - 1]\n\t\t\t\t\t\t\t\t\t];\n\t\t\t\t\t\t\t\tfotos.push({\n\t\t\t\t\t\t\